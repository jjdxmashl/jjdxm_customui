<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.dou361.demotest001"
          xmlns:android="http://schemas.android.com/apk/res/android">


    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.CustomSimpleActivity"/>
        <activity android:name=".activity.DatePickActivity"/>
        <activity android:name=".activity.PhotoActivity"/>
        <activity android:name=".activity.KeyBoardActivity"/>
        <activity android:name=".activity.StickEditTextActivity"/>
        <activity android:name=".activity.PulltoRefreshActivity"/>

    </application>

</manifest>
